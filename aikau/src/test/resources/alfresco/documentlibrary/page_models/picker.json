{
   "services": [
      {
         "name": "alfresco/services/LoggingService",
         "config": {
            "loggingPreferences": {
               "enabled": true,
               "all": true,
               "warn": true,
               "error": true
            }
         }
      },
      "alfresco/services/DocumentService",
      "alfresco/services/SearchService",
      "alfresco/services/ActionService",
