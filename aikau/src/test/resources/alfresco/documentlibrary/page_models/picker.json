            "showCancelButton": false,
            "widgets": [
               {
                  "name": "alfresco/forms/controls/DojoValidationTextBox",
                  "config": {
                     "label": "Search",
                     "name": "term"
                  }
               }
            ]
         }
      },
      {
         "name": "alfresco/search/AlfSearchList",
         "config": {
            "widgets" : [
