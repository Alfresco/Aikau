<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//www.tuckey.org//DTD UrlRewrite 3.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite use-query-string="true">

   <!-- Spring Surf -->
   <rule>
        <from>/favicon.ico</from>
        <to>/favicon.ico</to>
    </rule>
    <rule>
      <from>/proxy/(.*)</from>
      <to>/page/proxy/$1</to>
   </rule>
   <rule>
      <from>/res/(.*)</from>
      <to>/page/resource/$1</to>
   </rule>     
   <rule>
      <from>/service/(.*)</from>
      <to>/page/$1</to>
   </rule>        
   <rule>
      <from>/system/(.*)</from>
      <to>/page/system/$1</to>
    </rule>
    <rule>
      <from>/(.*)</from>
      <to>/page/$1</to>
   </rule>
   
</urlrewrite>